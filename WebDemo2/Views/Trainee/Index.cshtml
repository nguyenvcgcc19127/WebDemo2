@model IEnumerable<WebDemo2.Models.Trainee>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>
<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table class="table table-striped" id="table1">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Trainee_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Trainee_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Age)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date_of_Birth)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Education)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Password)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Trainee_ID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Trainee_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Age)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Date_of_Birth)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Education)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Password)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Trainee_ID }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Trainee_ID }, new { onclick = "return confirm('Are you sure you wish to delete this cat?');" })
                </td>
            </tr>
        }
    </tbody>
</table>

